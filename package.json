    "electron": "1.7.8",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^9.1.0",
    "grunt": "^1.0.1",
    "grunt-cli": "^1.2.0",
    "grunt-electron-installer": "^2.1.0",
    "less": "^2.6.1",
    "less-loader": "^2.2.2",
    "material-ui": "^0.17.1",
    "random-js": "^1.0.8",
    "react": "^15.4.2",
    "react-bootstrap-grid": "^1.0.0",
    "react-dom": "^15.4.2",
    "react-inline-grid": "^0.5.3",
    "react-tap-event-plugin": "^2.0.1",
